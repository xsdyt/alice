{
  "name" : "alice.playpeli.com",
  "description" : "The Laravel Framework.",
  "require" : {
    "php" : ">=5.5.9",
    "laravel/framework" : "5.1.*",
    "predis/predis" : "~1.1@dev"
  },
  "require-dev" : {
    "fzaninotto/faker" : "~1.4",
    "mockery/mockery" : "0.9.*",
    "phpunit/phpunit" : "~4.0",
    "phpspec/phpspec" : "~2.1",
    "maatwebsite/excel" : "~2.0"
  },
  "license" : "MIT",
  "keywords" : [ "framework", "laravel" ],
  "autoload-dev" : {
    "classmap" : [ "tests/TestCase.php" ]
  },
  "autoload" : {
    "classmap" : [ "database" ],
    "psr-4" : {
      "App\\" : "app/"
    },
    "files" : [ "app/Helpers/ApiHelper.php", "app/Helpers/IpHelper.php", "app/Helpers/BillingHelper.php", "app/Helpers/CmdHelper.php", "app/Helpers/RedisHelper.php", "app/Helpers/UtilsHelper.php" ]
  },
  "type" : "project",
  "scripts" : {
    "post-install-cmd" : [ "php artisan clear-compiled", "php artisan optimize" ],
    "pre-update-cmd" : [ "php artisan clear-compiled" ],
    "post-update-cmd" : [ "php artisan optimize" ],
    "post-root-package-install" : [ "php -r \"copy('.env.example', '.env');\"" ],
    "post-create-project-cmd" : [ "php artisan key:generate" ]
  },
  "config" : {
    "preferred-install" : "dist"
  }
}